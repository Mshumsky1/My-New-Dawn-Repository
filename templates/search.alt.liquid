{% layout none %}
{
  "data": [
{% paginate search.results by 999 %}
  {%- for item in search.results -%}
          {%- if item.object_type == 'article' -%}
              {% unless forloop.first %},{% endunless %}{
                "article": "{{ item.title }}",
                "url": "{{ item.url }}"
              }
          {%- endif -%}            
      {%- endfor -%}
{% endpaginate %}]
}